{
  "name": "pit-tool",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS='--inspect' next dev",
    "debug": "cross-env NODE_OPTIONS='--inspect' next",
    "build": "rimraf ./.next && next build",
    "start": "cross-env NODE_ENV=production next start -p $PORT",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "latest",
    "next-seo": "^4.26.0",
    "next-themes": "^0.0.15",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@types/node": "12.12.21",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/recharts": "^1.8.21",
    "autoprefixer": "10.2.1",
    "cross-env": "7.0.3",
    "husky": "5.1.1",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nesting": "^10.0.2",
    "postcss-preset-env": "^7.0.1",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "rimraf": "3.0.2",
    "tailwindcss": "^3.0.0",
    "typescript": "4.3.4"
  }
}
